{
  "name": "create-soldev-angular-app",
  "description": "Codebase builder for telkomsel angular app",
  "version": "0.1.5",
  "author": "stradivary",
  "bin": {
    "create-soldev-angular-app": "./bin/run.js"
  },
  "bugs": "https://github.com/Stradivary/create-soldev-angular-app/issues",
  "dependencies": {
    "@inquirer/prompts": "^5.3.8",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-version": "^2.2.10",
    "execa": "^9.3.1",
    "nanospinner": "^1.1.0",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist/**/*",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Stradivary/create-soldev-angular-app",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "create-soldev-angular-app",
    "dirname": "create-soldev-angular-app",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-version"
    ]
  },
  "repository": "Stradivary/create-soldev-angular-app",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "dev": "node ./bin/dev",
    "start": "node ./bin/run",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
